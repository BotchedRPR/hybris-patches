 #else
   CHECK_PTHREAD_CALL(pthread_setspecific, (Thread::pthread_key_self_, nullptr), "detach self");
@@ -1447,4 +1459,4 @@ ScopedSuspendAll::~ScopedSuspendAll() {
   Runtime::Current()->GetThreadList()->ResumeAll();
 }
 
